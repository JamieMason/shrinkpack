{
  "name": "shrinkpack",
  "description": "Fast, resilient, reproducible builds with npm install.",
  "version": "0.5.0",
  "author": "Jamie Mason <siunmaru@gmail.com>",
  "bin": {
    "shrinkpack": "index.js"
  },
  "bugs": {
    "url": "https://github.com/JamieMason/shrinkpack/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "contributors": [
    "Jason Hewison <jasonhewison@chunkysamurai.com>"
  ],
  "dependencies": {
    "chalk": "1.1.3",
    "is": "3.1.0",
    "lodash.uniq": "4.2.1",
    "when": "3.7.7"
  },
  "devDependencies": {
    "commit-release": "0.5.0",
    "cz-conventional-changelog": "1.1.5",
    "esformatter-literal-notation": "1.0.1",
    "esformatter-semicolon-first": "1.1.0",
    "esformatter-spaced-lined-comment": "2.0.1",
    "eslint": "2.6.0",
    "eslint-config-semistandard": "6.0.1",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-promise": "1.1.0",
    "eslint-plugin-react": "4.2.3",
    "eslint-plugin-standard": "1.3.2",
    "ghooks": "1.0.3",
    "semistandard-format": "2.1.0",
    "validate-commit-msg": "2.3.1"
  },
  "homepage": "https://github.com/JamieMason/shrinkpack",
  "keywords": [
    "bundle",
    "cache",
    "deploy",
    "deployments",
    "import",
    "install",
    "npm",
    "offline",
    "package",
    "save",
    "shrinkwrap"
  ],
  "license": "MIT",
  "main": "shrinkpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamieMason/shrinkpack.git"
  },
  "scripts": {
    "commit-release": "commit-release",
    "format": "semistandard-format -w",
    "lint": "eslint --config .eslintrc . --fix"
  }
}
